def age_check(age):
    print(f"you are {age}")
    
    if age<18:
        print("you cannot drink")
    elif age==18 or age==19:
        print("you are new to this")
        
    elif age>=20 and age<25:
        print("Just Do It")
    else:
        print("enjoy your drink")

age_check(15)
age_check(18)
age_check(20)
age_check(26)

you are 15
you cannot drink
you are 18
you are new to this
you are 20
Just Do It
you are 26
enjoy your drink
